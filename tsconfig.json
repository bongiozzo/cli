{
  "extends": "@diplodoc/tsconfig",
  "compilerOptions": {
    "lib": ["ES2019"],
    "target": "ES2019",
    "outDir": "build",
    "module": "preserve",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"],
  "types": ["node"]
}
